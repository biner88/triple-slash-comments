{
  "name": "tsc",
  "displayName": "TSC",
  "description": "Triple Slash Comments",
  "publisher": "Teenagex",
  "author": {
    "name": "Teenagex"
  },
  "version": "1.0.0",
  "engines": {
    "vscode": "^1.80.0"
  },
  "categories": [
    "Formatters"
  ],
  "galleryBanner": {
    "color": "#F2F2F2",
    "theme": "light"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/biner88/triple-slash-comments"
  },
  "bugs": {
    "url": "https://github.com/biner88/triple-slash-comments/issues"
  },
  "keywords": [
    "Triple Slash Comments",
    "Comments",
    "dart"
  ],
  "icon": "images/AppIcon.png",
  "activationEvents": [],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "tsc.triplesSlashComments",
        "title": "Triple Slash Comments"
      }
    ]
  },
  "keybindings": [
    {
      "command": "tsc.triplesSlashComments",
      "key": "ctrl+/ ctrl+/",
      "mac": "cmd+/ cmd+/",
      "when": "editorTextFocus"
    }
  ],
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "node ./test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.80.0",
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "20.2.5",
    "eslint": "^8.41.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "typescript": "^5.1.3",
    "@vscode/test-electron": "^2.3.2"
  }
}